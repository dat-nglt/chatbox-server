{
    "name": "chatbox-server",
    "version": "1.0.0",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "dev": "nodemon --exec babel-node src/server.js",
        "start": "node dist/server.js",
        "build": "npm run clean && babel src --out-dir dist --copy-files",
        "clean": "rm -rf dist",
        "db:create": "sequelize-cli db:create",
        "db:migrate": "sequelize-cli db:migrate",
        "db:seed": "sequelize-cli db:seed:all"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "Chatbox server for Zalo Mini App",
    "dependencies": {
        "@google/genai": "^1.29.0",
        "axios": "^1.13.2",
        "bullmq": "^5.63.0",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^4.18.2",
        "express-rate-limit": "^6.8.1",
        "form-data": "^4.0.5",
        "googleapis": "^166.0.0",
        "helmet": "^7.0.0",
        "joi": "^17.9.2",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.1",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.32.1",
        "socket.io": "^4.8.1",
        "uuid": "^13.0.0",
        "winston": "^3.18.3",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.28.3",
        "@babel/core": "^7.28.5",
        "@babel/node": "^7.28.0",
        "@babel/preset-env": "^7.28.5",
        "nodemon": "^3.1.11",
        "sequelize-cli": "^6.6.2"
    }
}
